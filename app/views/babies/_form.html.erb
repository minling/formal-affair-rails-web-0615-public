<!-- Your form goes here! -->
<%= form_for(@baby) do |f| %>
<input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" />

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :weight %>
  <%= f.text_field :weight %>

  <%= f.label :birth_date %>
  <%= f.date_field :birth_date %>

  <%= f.submit %>
<% end %>